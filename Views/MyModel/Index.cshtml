@model IEnumerable<MyModel>

@{
    ViewData["Title"] = "MyModel";
}

<h1>Hello MyModel!</h1>



@if (Model.Count() > 0)
{
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>Name</th>
                <th>Nymber</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td width="50%">@item.Name</td>
                    <td width="30%">@item.Number</td>
                    <td width="20%"> - </td>
                </tr>
            }
        </tbody>
    </table>
}
